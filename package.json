{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.css -o public/css/style.prefix.css",
    "compress:sass" : "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "start": "react-scripts start ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm start\" \"npm run watch:sass\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "node-sass": "^4.14.1",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1"
  }
}
